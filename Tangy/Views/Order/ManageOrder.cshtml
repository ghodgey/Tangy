@model List<Tangy.Models.OrderDetailsViewModels.OrderDetailsViewModel>


@{
    ViewData["Title"] = "ManageOrder";
}

<h2>Manage Orders</h2>

<form method="post">
    <br />

    @if(Model.Count > 0)
    { 
        @foreach(var item in Model)
        {
            <div class="container row">
                <div class="col-10">
                    @Html.Partial("_IndividualOrder", item)
                </div>
                <div class="col-2">

                </div>
            </div>

        }
    }
</form>

