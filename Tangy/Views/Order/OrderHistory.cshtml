@model List<Tangy.Models.OrderDetailsViewModels.OrderDetailsViewModel>


@{
    ViewData["Title"] = "OrderHistory";
}

<h2>Order History</h2>
<br />

@if(Model.Count > 0)
{
    @foreach(var item in Model)
    {
        @Html.Partial("_IndividualOrder", item)

    }

}
else
{
    <p>No past orders available...</p>

}

